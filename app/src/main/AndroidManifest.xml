<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.kks.portfolio_android"
    android:targetSandboxVersion="1">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_main"
        android:label="WASSUP?"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@mipmap/ic_main_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".activity.PostLikeUserActivity"></activity>
        <activity android:name=".activity.PageActivity" />
        <activity android:name=".follow.Following_Activity" />
        <activity android:name=".follow.Follower_Activity" />
        <activity android:name=".search.Search_PostingResult" />
        <activity android:name=".search.Search_UserResult" />
        <activity android:name=".activity.PostingActivity" />
        <activity android:name=".activity.CommentActivity" />
        <activity android:name=".activity.SettingActivity" />
        <activity android:name=".activity.HomeActivity" />
        <activity android:name=".activity.Sign_upActivity" />
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <uses-library
            android:name="org.apache.http.legacy"
            android:required="false" />
    </application>

</manifest>